{
  "manifest_version": 3,
  "name": "Flag Icon Switcher",
  "version": "1.0",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": {
      "16": "icons/default.png"
    },
    "default_title": "Flag Icon Switcher"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/default.png"
  }
}
