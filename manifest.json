{
  "manifest_version": 2,
  "name": "Flag Icon Switcher",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/default.png",
    "default_title": "Flag Icon Switcher"
  },
  "permissions": ["activeTab"]
}
